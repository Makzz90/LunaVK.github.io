    <ResourceDictionary.ThemeDictionaries>
        <ResourceDictionary x:Key="Light">
            <!-- 100% акцентирующий цвет (Синий) -->
            <SolidColorBrush x:Key="AccentBrushHigh" Color="#2196f3" />

            <!-- 80% акцентирующий цвет -->
            <SolidColorBrush x:Key="AccentBrushMediumHigh" Color="#FF6389CD" />

            <!-- 60% акцентирующий цвет -->
            <SolidColorBrush x:Key="AccentBrushMedium" Color="#FF79A0E6" />

